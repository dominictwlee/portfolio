{
	"extends": [
		"airbnb",
		"prettier",
		"prettier/react",
		"plugin:css-modules/recommended"
	],
	"plugins": [
		"prettier",
		"css-modules"
	],
	"parserOptions": {
		"ecmaVersion": 2016,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	}
}
